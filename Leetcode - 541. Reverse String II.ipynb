   "source": [
    "class Solution(object):\n",
    "    def reverseStr(self, s, k):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :type k: int\n",
    "        :rtype: str\n",
    "        \"\"\"\n",
    "        ans=\"\"\n",
    "        for i in range(0,len(s)+2*k,2*k):\n",
    "            ans=ans+s[i:i+k][::-1]+s[i+k:i+2*k]\n",
    "        return ans"
   ]
